@model HandleErrorInfo
@{
    this.ViewBag.Title = "Error page";
    this.Layout = "~/Views/Shared/_SiteLayout.cshtml";

}



<div class="content">
    <h1>Error
    </h1>
    <div>
        An error occurred while processing your request.
    </div>
    <div>
        @this.Model.Exception.Message
    </div>
</div>

